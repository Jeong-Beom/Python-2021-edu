    "welcome to the new world''' #여러줄의 문자열 변수 작성 및 부여\r\n",
    "print(x)\r\n",
    "del x\r\n",
    "\r\n",
    "# 문자열작성시 ' 및 \"사용하기\r\n",
    "x = \"Python isn't difficult\"\r\n",
